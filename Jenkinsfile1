pipeline{
    agent any
    tools {
        maven 'Default Maven'
        jdk 'Java SE'
    }
    stages{
    
        stage('Git clone'){
            steps{
                 git branch: 'master',git changelog: false, credentialsId: 'f9dcdd6d-8985-49ef-b10a-c17a8e9fdb8b', poll: false, url: 'https://github.com/Devanshc12/my-app1.git'
             }
        }
        
        stage('Build'){
             steps{
                 bat 'mvn clean install'
                 }
        }
        
        stage('SonarQube Analysis'){
            steps{
                withSonarQubeEnv('Sonar scanner'){
                    bat "mvn sonar:sonar"
                 }
            }
        }
     }
}     
     
        
